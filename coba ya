function popupku() {
	var body = document.querySelector("body");
	var pWm = document.createElement("a");
	pWm.setAttribute("href", "https://twitter.com/dandelionriz");
	pWm.setAttribute("style", "text-decoration: none; color: blue; opacity: 0; position: fixed; bottom : 10px; left: 10px");
	pWm.innerHTML = "twitter : dandelionriz";
	body.appendChild(pWm);
}	
